models/players/protocol/protocol_leg
{
    {
        map models/players/protocol/protocol_leg
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/protocol/default_reflect5
        blendFunc GL_DST_COLOR GL_ONE
        tcGen environment
    }
    {
        map models/players/protocol/protocol_leg_noshine
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        detail
        rgbGen lightingDiffuse
    }
    {
        map models/players/protocol/protocol_leg_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

