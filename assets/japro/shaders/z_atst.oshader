models/players/atst/atst_material
{
    {
        map models/map_objects/imp_detention/atst_diffuse
        rgbGen lightingDiffuse
    }
    {
        map models/map_objects/imp_detention/atst_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

